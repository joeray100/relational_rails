<h1>New Menu Item</h1>
<%= form_with url: "/bakeries/:id/pastries", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %><br><br>
  <%= form.label :price%>
  <%= form.text_field :price %><br><br>
  <%= form.label :gluten_free%>
  <%= form.text_field :gluten_free%><br><br>
  <%= form.submit 'Create New Item' %>
<% end %>
